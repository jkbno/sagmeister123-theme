{% style %}
  @font-face {
    font-family: "ReplicaStd-Bold";
    src: url('{{ 'ReplicaStd-Bold.static.ttf' | asset_url }}') format('truetype');
  }
  @font-face {
    font-family: "ReplicaStd-Regular";
    src: url('{{ 'ReplicaStd-Regular.static.ttf' | asset_url }}') format('truetype');
  }
  @font-face {
    font-family: "ReplicaStd-Light";
    src: url('{{ 'ReplicaStd-Light.static.ttf' | asset_url }}') format('truetype');
  }
  @font-face {
    font-family: "GT-Sectra-Display-Regular";
    src: url('{{ 'GT-Sectra-Display-Regular.static.ttf' | asset_url }}') format('truetype');
  }
  @font-face {
    font-family: "GT-Sectra-Display-Bold";
    src: url('{{ 'GT-Sectra-Display-Bold.static.ttf' | asset_url }}') format('truetype');
  }
{% endstyle %}