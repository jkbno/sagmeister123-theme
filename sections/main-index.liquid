<home-sections-home-video-header
  :home-video-data="{
    homeVideoDesktopUrl: `{{ homeVideoDesktopUrl }}`,
    homeVideoPosterDesktopUrl: `{{ homeVideoPosterDesktopUrl }}`,
    homeVideoMobileUrl: `{{ homeVideoMobileUrl }}`,
    homeVideoPosterMobilepUrl: `{{ homeVideoPosterMobilepUrl }}`,
  }"
>
</home-sections-home-video-header>
<home-sections-home-now-is-better
  :now-is-better-data="{
    nowIsBetter: `{{ 'home.nowIsBetter' | t }}`,
    scrollAnimationFirst: `{{ 'home.scrollAnimationFirstLine' | t }}`,
    scrollAnimationSecond: `{{ 'home.scrollAnimationSecondLine' | t }}`,
  }"
>
</home-sections-home-now-is-better>
<home-sections-home-collection
  :collection-data="{
    collection: `{{ 'home.collection' | t }}`,
    collectionUrl: `{{ settings.collectionUrl }}`,
    isWhite: `{{ collectionIsWhite }}`,
    look: `{{ 'home.look' | t }}`,
    productImages: {
      artMarketTShirt: {
        videoUrl: `{{ artMarketTShirtVideoUrl }}`,
        videoPosterUrl: `{{ artMarketTShirtPosterUrl }}`,
        imageHoverSrc: `{{ all_products["art-market-t-shirt"].images.last | image_url }}`,
        url: `{{ settings.artMarketTShirtUrl }}`,
      },
      artistsTShirt: {
        videoUrl: `{{ artistsTShirtVideoUrl }}`,
        videoPosterUrl: `{{ artistsTShirtPosterUrl }}`,
        imageHoverSrc: `{{ all_products["artists-t-shirt"].images.last | image_url }}`,
        url: `{{ settings.artistsTShirtUrl }}`,
      },
      blowerShirt: {
        videoUrl: `{{ blowerShirtVideoUrl }}`,
        videoPosterUrl: `{{ blowerShirtPosterUrl }}`,
        imageHoverSrc: `{{ all_products["blower-shirt"].images.last | image_url }}`,
        url: `{{ settings.blowerShirtUrl }}`,
      },
      democraticSweater: {
        videoUrl: `{{ democraticSweaterVideoUrl }}`,
        videoPosterUrl: `{{ democraticSweaterPosterUrl }}`,
        imageHoverSrc: `{{ all_products["democratic-sweater"].images.last | image_url }}`,
        url: `{{ settings.democraticSweaterUrl }}`,
      },
      murderousCoat: {
        videoUrl: `{{ murderousCoatVideoUrl }}`,
        videoPosterUrl: `{{ murderousCoatPosterUrl }}`,
        imageHoverSrc: `{{ all_products["murderous-coat"].images.last | image_url }}`,
        url: `{{ settings.murderousCoatUrl }}`,
      },
      naturalDisasterLongSleeve: {
        videoUrl: `{{ naturalDisasterLongSleeveVideoUrl }}`,
        videoPosterUrl: `{{ naturalDisasterLongSleevePosterUrl }}`,
        imageHoverSrc: `{{ all_products["disaster-long-sleeve"].images.last | image_url }}`,
        url: `{{ settings.naturalDisasterLongSleeveUrl }}`,
      },
    }
  }"
>
</home-sections-home-collection>
<home-sections-text-image-blocks
  :blocks-data="[
    {
      showButton: `{{ showButtonBeutifulNumbers }}`,
      dashedButtonData: {
        isWhite: `{{ buttonBeautifulNumbersIsWhite }}`,
        text: `{{ 'home.beautifulNumbersButton' | t }}`,
        url: `{{ settings.beautifulNumbersUrl }}`,
      },
      textImageBlockClass: `{{ textImageBlockPink }}`,
      imageUrl: `{{ infoBeautifulNumbersDataImageUrl }}`,
      mainText: `{{ 'home.beautifulNumbersText' | t }}`,
    },
    {
      showButton: `{{ showButtonAboutUs }}`,
      dashedButtonData: {
        isWhite: `{{ buttonAboutUsIsWhite }}`,
        text: `{{ 'home.aboutUsButton' | t }}`,
        url: `{{ settings.aboutUrl }}`,
      },
      ifTextRight: `{{ ifTextRight }}`,
      textImageBlockClass: `{{ textImageBlockBlue }}`,
      imageUrl: `{{ infoGalleryDataImageUrl }}`,
      mainText: `{{ 'home.aboutUsText' | t }}`,
    },
  ]"
>
</home-sections-text-image-blocks>
