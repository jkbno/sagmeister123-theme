{% assign isTransparentNav = 'true' %}

{% assign showButtonBeutifulNumbers = 'true' %}
{% assign showButtonAboutUs = 'true' %}
{% assign collectionIsWhite = 'true' %}
{% assign buttonBeautifulNumbersIsWhite = 'false' %}
{% assign buttonAboutUsIsWhite = 'false' %}
{% assign textImageBlockBlue = 'text-image-block_blue' %}
{% assign col6Lg = 'col-6-lg' %}
{% assign col3Lg = 'col-3-lg' %}
{% assign textImageBlockYellow = 'text-image-block_yellow' %}
{% assign ifTextRight = 'true' %}
{% assign ifNotTextRight = 'false' %}

{% assign infoBeautifulNumbersDataImageUrl = 'https://cdn.shopify.com/s/files/1/0572/4861/2546/files/Painting-BeautifulNumbers-HP.jpg?v=1646867820' %}
{% assign infoGalleryDataImageUrl = 'https://cdn.shopify.com/s/files/1/0572/4861/2546/files/LIGHTNING_JACKET_M_0094_HP.jpg?v=1651685567' %}

{% assign homeVideoDesktopUrl = 'https://cdn.shopify.com/s/files/1/0572/4861/2546/files/123_Video_Desktop.mp4?v=1646531058' %}
{% assign homeVideoPosterDesktopUrl = 'https://cdn.shopify.com/s/files/1/0572/4861/2546/files/123_Video_Desktop.jpg?v=1646532625' %}
{% assign homeVideoMobileUrl = 'https://cdn.shopify.com/s/files/1/0572/4861/2546/files/123_Video_Mobile_double.mp4?v=1647041942' %}
{% assign homeVideoPosterMobilepUrl = 'https://cdn.shopify.com/s/files/1/0572/4861/2546/files/123_Video_Mobile_double.jpg?v=1647041938' %}

{% render 'navigation-bar', isTransparentNav: isTransparentNav %}
<home-sections-home-video-header
  :home-video-data="{
    homeVideoDesktopUrl: `{{ homeVideoDesktopUrl }}`,
    homeVideoPosterDesktopUrl: `{{ homeVideoPosterDesktopUrl }}`,
    homeVideoMobileUrl: `{{ homeVideoMobileUrl }}`,
    homeVideoPosterMobilepUrl: `{{ homeVideoPosterMobilepUrl }}`,
  }"
>
</home-sections-home-video-header>
<home-sections-home-now-is-better
  :now-is-better-data="{
    nowIsBetter: `{{ 'home.nowIsBetter' | t }}`,
    scrollAnimationFirst: `{{ 'home.scrollAnimationFirstLine' | t }}`,
    scrollAnimationSecond: `{{ 'home.scrollAnimationSecondLine' | t }}`,
  }"
>
</home-sections-home-now-is-better>
<home-sections-home-collection
  :collection-data="{
    collection: `{{ 'home.collection' | t }}`,
    collectionUrl: `{{ settings.collectionUrl }}`,
    isWhite: `{{ collectionIsWhite }}`,
    look: `{{ 'home.look' | t }}`,
    productImages: {
      lifeShirt: {
        imageMainSrc: `{{ all_products["life-shirt"].images.first | image_url }}`,
        imageHoverSrc: `{{ all_products["life-shirt"].images.last | image_url }}`,
        url: `{{ settings.productsLifeShirtUrl }}`,
      },
      lightningJacket: {
        imageMainSrc: `{{ all_products["lightning-jacket"].images.first | image_url }}`,
        imageHoverSrc: `{{ all_products["lightning-jacket"].images.last | image_url }}`,
        url: `{{ settings.productsLightningJacketUrl }}`,
      },
      marathonHoodie: {
        imageMainSrc: `{{ all_products["marathon-hoodie"].images.first | image_url }}`,
        imageHoverSrc: `{{ all_products["marathon-hoodie"].images.last | image_url }}`,
        url: `{{ settings.productsMarathonHoodieUrl }}`,
      },
      opinionCoat: {
        imageMainSrc: `{{ all_products["opinion-coat"].images.first | image_url }}`,
        imageHoverSrc: `{{ all_products["opinion-coat"].images.last | image_url }}`,
        url: `{{ settings.productsOpinionCoatUrl }}`,
      },
      pedestrianSweater: {
        imageMainSrc: `{{ all_products["pedestrian-sweater"].images.first | image_url }}`,
        imageHoverSrc: `{{ all_products["pedestrian-sweater"].images.last | image_url }}`,
        url: `{{ settings.productsPedestrianSweaterUrl }}`,
      },
      progressShirt: {
        imageMainSrc: `{{ all_products["progress-shirt"].images.first | image_url }}`,
        imageHoverSrc: `{{ all_products["progress-shirt"].images.last | image_url }}`,
        url: `{{ settings.productsProgressShirtUrl }}`,
      },
    }
  }"
>
</home-sections-home-collection>
<home-sections-text-image-blocks
  :blocks-data="[
    {
      showButton: `{{ showButtonBeutifulNumbers }}`,
      dashedButtonData: {
        isWhite: `{{ buttonBeautifulNumbersIsWhite }}`,
        text: `{{ 'home.beautifulNumbersButton' | t }}`,
        url: `{{ settings.beautifulNumbersUrl }}`,
      },
      textImageBlockClass: `{{ textImageBlockYellow }}`,
      textImageBlockColLeftClass: `{{ col3Lg }}`,
      textImageBlockColRightClass: `{{ col6Lg }}`,
      ifTextRight: `{{ ifTextRight }}`,
      imageUrl: `{{ infoBeautifulNumbersDataImageUrl }}`,
      mainText: `{{ 'home.beautifulNumbersText' | t }}`,
    },
    {
      showButton: `{{ showButtonAboutUs }}`,
      dashedButtonData: {
        isWhite: `{{ buttonAboutUsIsWhite }}`,
        text: `{{ 'home.aboutUsButton' | t }}`,
        url: `{{ settings.aboutUrl }}`,
      },
      textImageBlockClass: `{{ textImageBlockBlue }}`,
      textImageBlockColLeftClass: `{{ col6Lg }}`,
      textImageBlockColRightClass: `{{ col3Lg }}`,
      ifTextRight: `{{ ifNotTextRight }}`,
      imageUrl: `{{ infoGalleryDataImageUrl }}`,
      mainText: `{{ 'home.aboutUsText' | t }}`,
    },
  ]"
>
</home-sections-text-image-blocks>
{% comment %} <global-sections-global-narrow-video
  :color="darkGreen"
>
</global-sections-global-narrow-video> {% endcomment %}
<global-sections-global-good-news
  :good-news-data="{
    goodNews: `{{ 'home.goodNews' | t }}`,
    products: {
      lifeShirt: {
        title: `{{ all_products["life-shirt"].title }}`,
        price: `{{ all_products["life-shirt"].price | money_without_trailing_zeros }}`,
        imgUrl: `{{ all_products["life-shirt"].images[0] | image_url }}`,
        productUrl: `{{ settings.productsLifeShirtUrl }}`,
      },
      lightningJacket: {
        title: `{{ all_products["lightning-jacket"].title }}`,
        price: `{{ all_products["lightning-jacket"].price | money_without_trailing_zeros }}`,
        imgUrl: `{{ all_products["lightning-jacket"].images[0] | image_url }}`,
        productUrl: `{{ settings.productsLightningJacketUrl }}`,
      },
      marathonHoodie: {
        title: `{{ all_products["marathon-hoodie"].title }}`,
        price: `{{ all_products["marathon-hoodie"].price | money_without_trailing_zeros }}`,
        imgUrl: `{{ all_products["marathon-hoodie"].images[0] | image_url }}`,
        productUrl: `{{ settings.productsMarathonHoodieUrl }}`,
      },
      opinionCoat: {
        title: `{{ all_products["opinion-coat"].title }}`,
        price: `{{ all_products["opinion-coat"].price | money_without_trailing_zeros }}`,
        imgUrl: `{{ all_products["opinion-coat"].images[0] | image_url }}`,
        productUrl: `{{ settings.productsOpinionCoatUrl }}`,
      },
      pedestrianSweater: {
        title: `{{ all_products["pedestrian-sweater"].title }}`,
        price: `{{ all_products["pedestrian-sweater"].price | money_without_trailing_zeros }}`,
        imgUrl: `{{ all_products["pedestrian-sweater"].images[0] | image_url }}`,
        productUrl: `{{ settings.productsPedestrianSweaterUrl }}`,
      },
      progressShirt: {
        title: `{{ all_products["progress-shirt"].title }}`,
        price: `{{ all_products["progress-shirt"].price | money_without_trailing_zeros }}`,
        imgUrl: `{{ all_products["progress-shirt"].images[0] | image_url }}`,
        productUrl: `{{ settings.productsProgressShirtUrl }}`,
      },
    },
  }"
>
</global-sections-global-good-news>
{% render 'footer' %}
{% render 'cookie-banner' %}
{% render 'idle-eucalyptus' %}

  {% schema %}
{
  "name": "Main index",
  "class": "vue-section",
  "presets": [
    {
      "name": "main index"
    }
  ]
}
{% endschema %}